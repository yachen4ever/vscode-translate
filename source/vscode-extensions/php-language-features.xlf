  <file original="extensions/php-language-features/package" source-language="en" datatype="plaintext"><body>
    <trans-unit id="configuration.suggest.basic">
      <source xml:lang="en">Controls whether the built-in PHP language suggestions are enabled. The support suggests PHP globals and variables.</source>
    </trans-unit>
    <trans-unit id="configuration.validate.enable">
      <source xml:lang="en">Enable/disable built-in PHP validation.</source>
    </trans-unit>
    <trans-unit id="configuration.validate.executablePath">
      <source xml:lang="en">Points to the PHP executable.</source>
    </trans-unit>
    <trans-unit id="configuration.validate.run">
      <source xml:lang="en">Whether the linter is run on save or on type.</source>
    </trans-unit>
    <trans-unit id="configuration.title">
      <source xml:lang="en">PHP</source>
    </trans-unit>
    <trans-unit id="commands.categroy.php">
      <source xml:lang="en">PHP</source>
    </trans-unit>
    <trans-unit id="command.untrustValidationExecutable">
      <source xml:lang="en">Disallow PHP validation executable (defined as workspace setting)</source>
    </trans-unit>
    <trans-unit id="displayName">
      <source xml:lang="en">PHP Language Features</source>
    </trans-unit>
    <trans-unit id="description">
      <source xml:lang="en">Provides rich language support for PHP files.</source>
    </trans-unit>
  </body></file>
  <file original="extensions/php-language-features/dist/features/validationProvider" source-language="en" datatype="plaintext"><body>
    <trans-unit id="php.useExecutablePath">
      <source xml:lang="en">Do you allow {0} (defined as a workspace setting) to be executed to lint PHP files?</source>
    </trans-unit>
    <trans-unit id="php.yes">
      <source xml:lang="en">Allow</source>
    </trans-unit>
    <trans-unit id="php.no">
      <source xml:lang="en">Disallow</source>
    </trans-unit>
    <trans-unit id="wrongExecutable">
      <source xml:lang="en">Cannot validate since {0} is not a valid php executable. Use the setting 'php.validate.executablePath' to configure the PHP executable.</source>
    </trans-unit>
    <trans-unit id="noExecutable">
      <source xml:lang="en">Cannot validate since no PHP executable is set. Use the setting 'php.validate.executablePath' to configure the PHP executable.</source>
    </trans-unit>
    <trans-unit id="unknownReason">
      <source xml:lang="en">Failed to run php using path: {0}. Reason is unknown.</source>
    </trans-unit>
    <trans-unit id="goToSetting">
      <source xml:lang="en">Open Settings</source>
    </trans-unit>
  </body></file>
