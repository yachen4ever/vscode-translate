    <trans-unit id="description">
      <source xml:lang="en">Extension to add task support for npm scripts.</source>
    </trans-unit>
    <trans-unit id="displayName">
      <source xml:lang="en">NPM support for VS Code</source>
    </trans-unit>
    <trans-unit id="config.npm.autoDetect">
