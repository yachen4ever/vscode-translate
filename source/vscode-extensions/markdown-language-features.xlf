<?xml version="1.0" encoding="utf-8"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
  <file original="extensions/markdown-language-features/package" source-language="en" datatype="plaintext"><body>
    <trans-unit id="displayName">
      <source xml:lang="en">Markdown Language Features</source>
    </trans-unit>
    <trans-unit id="description">
      <source xml:lang="en">Provides rich language support for Markdown.</source>
    </trans-unit>
    <trans-unit id="markdown.preview.breaks.desc">
      <source xml:lang="en">Sets how line-breaks are rendered in the markdown preview. Setting it to 'true' creates a &lt;br&gt; for every newline.</source>
    </trans-unit>
    <trans-unit id="markdown.preview.linkify">
      <source xml:lang="en">Enable or disable conversion of URL-like text to links in the markdown preview.</source>
    </trans-unit>
    <trans-unit id="markdown.preview.doubleClickToSwitchToEditor.desc">
      <source xml:lang="en">Double click in the markdown preview to switch to the editor.</source>
    </trans-unit>
    <trans-unit id="markdown.preview.fontFamily.desc">
      <source xml:lang="en">Controls the font family used in the markdown preview.</source>
    </trans-unit>
    <trans-unit id="markdown.preview.fontSize.desc">
      <source xml:lang="en">Controls the font size in pixels used in the markdown preview.</source>
    </trans-unit>
    <trans-unit id="markdown.preview.lineHeight.desc">
      <source xml:lang="en">Controls the line height used in the markdown preview. This number is relative to the font size.</source>
    </trans-unit>
    <trans-unit id="markdown.preview.markEditorSelection.desc">
      <source xml:lang="en">Mark the current editor selection in the markdown preview.</source>
    </trans-unit>
    <trans-unit id="markdown.preview.scrollEditorWithPreview.desc">
      <source xml:lang="en">When a markdown preview is scrolled, update the view of the editor.</source>
    </trans-unit>
    <trans-unit id="markdown.preview.scrollPreviewWithEditor.desc">
      <source xml:lang="en">When a markdown editor is scrolled, update the view of the preview.</source>
    </trans-unit>
    <trans-unit id="markdown.preview.title">
      <source xml:lang="en">Open Preview</source>
    </trans-unit>
    <trans-unit id="markdown.previewSide.title">
      <source xml:lang="en">Open Preview to the Side</source>
    </trans-unit>
    <trans-unit id="markdown.showLockedPreviewToSide.title">
      <source xml:lang="en">Open Locked Preview to the Side</source>
    </trans-unit>
    <trans-unit id="markdown.showSource.title">
      <source xml:lang="en">Show Source</source>
    </trans-unit>
    <trans-unit id="markdown.styles.dec">
      <source xml:lang="en">A list of URLs or local paths to CSS style sheets to use from the markdown preview. Relative paths are interpreted relative to the folder open in the explorer. If there is no open folder, they are interpreted relative to the location of the markdown file. All '\' need to be written as '\\'.</source>
    </trans-unit>
    <trans-unit id="markdown.showPreviewSecuritySelector.title">
      <source xml:lang="en">Change Preview Security Settings</source>
    </trans-unit>
    <trans-unit id="markdown.trace.desc">
      <source xml:lang="en">Enable debug logging for the markdown extension.</source>
    </trans-unit>
    <trans-unit id="markdown.preview.refresh.title">
      <source xml:lang="en">Refresh Preview</source>
    </trans-unit>
    <trans-unit id="markdown.preview.toggleLock.title">
      <source xml:lang="en">Toggle Preview Locking</source>
    </trans-unit>
    <trans-unit id="configuration.markdown.preview.openMarkdownLinks.description">
      <source xml:lang="en">Controls how links to other markdown files in the markdown preview should be opened.</source>
    </trans-unit>
    <trans-unit id="configuration.markdown.preview.openMarkdownLinks.inEditor">
      <source xml:lang="en">Try to open links in the editor</source>
    </trans-unit>
    <trans-unit id="configuration.markdown.preview.openMarkdownLinks.inPreview">
      <source xml:lang="en">Try to open links in the markdown preview</source>
    </trans-unit>
    <trans-unit id="configuration.markdown.links.openLocation.description">
      <source xml:lang="en">Controls where links in markdown files should be opened.</source>
    </trans-unit>
    <trans-unit id="configuration.markdown.links.openLocation.currentGroup">
      <source xml:lang="en">Open links in the active editor group.</source>
    </trans-unit>
    <trans-unit id="configuration.markdown.links.openLocation.beside">
      <source xml:lang="en">Open links beside the active editor.</source>
    </trans-unit>
  </body></file>
